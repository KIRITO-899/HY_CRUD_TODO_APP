<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
    <msapplication>
        <tile>
            <square70x70logo src="/favicon-32x32.svg"/>
            <square150x150logo src="/favicon.svg"/>
            <square310x310logo src="/apple-touch-icon.svg"/>
            <TileColor>#4F46E5</TileColor>
        </tile>
        <notification>
            <polling-uri src="/api/notifications/"/>
            <frequency>30</frequency>
            <cycle>1</cycle>
        </notification>
    </msapplication>
</browserconfig>
